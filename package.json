{
  "name": "react-redliner",
  "version": "0.0.69",
  "description": "A React utility to display redlines",
  "keywords": [
    "react",
    "redliner",
    "measure",
    "style",
    "attributes"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build:docz": "docz build",
    "build:npm": "rm -rf ./lib && tsc",
    "clean:docz": "rm -rf .docz",
    "format": "pretty-quick --staged --pattern \"**/*.*(js|jsx|ts|tsx)\"",
    "prepublishOnly": "yarn build:npm",
    "start": "docz dev"
  },
  "contributors": [
    {
      "name": "Veronica Carrillo-Marquez"
    },
    {
      "name": "Tim Rybicki"
    }
  ],
  "license": "MIT",
  "homepage": "https://redliner.f1v.co",
  "repository": {
    "type": "git",
    "url": "https://github.com/f1v/redliner.git"
  },
  "bugs": {
    "url": "https://github.com/f1v/redliner/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-popper": "^1.3.7",
    "react-popper-tooltip": "^2.10.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.12.0",
    "@types/react-dom": "^16.12.0",
    "docz": "2.2.0",
    "gatsby": "^2.19.8",
    "gatsby-plugin-emotion": "^4.3.1",
    "gatsby-theme-docz": "^2.2.0",
    "husky": "^4.2.1",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "resolutions": {
    "**/react": "^16.12.0",
    "**/react-dom": "^16.12.0"
  }
}
