{
  "name": "docz-example-typescript",
  "version": "0.2",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project ./tsconfig.json && yarn rollup",
    "clean": "rm -rf .docz node_modules yarn.lock",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "rollup": "rollup ./lib/RedLiner/RedLiner.js --file ./lib/index.js --format iife",
    "start": "docz dev"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "classnames": "^2.2.6",
    "docz": "2.2.0",
    "gatsby-plugin-sass": "^2.1.27",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-popper": "^1.3.7"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "docz-theme-default": "^1.0.4",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.19",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
