{
  "name": "@f1v/redliner",
  "version": "0.0.1",
  "description": "A React utility to display redlines",
  "main": "lib/index.js",
  "scripts": {
    "build:rollup": "tsc --project ./tsconfig.json && yarn rollup",
    "build": "docz build",
    "clean": "rm -rf .docz node_modules yarn.lock",
    "rollup": "rollup ./lib/RedLiner/RedLiner.js --file ./lib/index.js --format iife",
    "start": "docz dev"
  },
  "contributors": [
    {
      "name": "Veronica Carrillo-Marquez"
    },
    {
      "name": "Tim Rybicki"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/f1v/redliner.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.19.8",
    "gatsby-theme-docz": "^2.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.4",
    "react-popper": "^1.3.7",
    "react-popper-tooltip": "^2.10.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "docz": "2.2.0",
    "gatsby-plugin-sass": "^2.1.27",
    "husky": "^4.2.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
