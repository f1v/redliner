{
  "name": "@f1v/redliner",
  "version": "0.0.1",
  "description": "A React utility to display redlines",
  "main": "lib/index.js",
  "scripts": {
    "build:rollup": "tsc --project ./tsconfig.json && yarn rollup",
    "build": "docz build",
    "clean": "rm -rf lib .docz node_modules yarn.lock",
    "rollup": "rollup ./lib/RedLiner/RedLiner.js --file ./lib/index.js --format iife",
    "start": "docz dev"
  },
  "author": "Veronica Carrillo-Marquez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/f1v/redliner.git"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.9.0",
    "react-dom": "^16.8.6",
    "react-popper": "^1.3.7",
    "react-popper-tooltip": "^2.10.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "docz": "2.2.0",
    "gatsby-plugin-sass": "^2.1.27",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
